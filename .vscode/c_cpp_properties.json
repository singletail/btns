{
    "env": {
        "myIncludePath": [
            "${workspaceFolder}/main/config/include",
            "${workspaceFolder}/main/core/include",
            "${workspaceFolder}/main/util/include",
            "${workspaceFolder}/main/net/include"
        ]
    },
    "configurations": [
        {
            "name": "ESP-IDF",
            "compilerPath": "${config:idf.toolsPath}/tools/xtensa-esp-elf/esp-14.2.0_20241119/xtensa-esp-elf/bin/xtensa-esp32-elf-gcc",
            "compileCommands": "${config:idf.buildPath}/compile_commands.json",
            "intelliSenseMode": "macos-gcc-arm64",
            "cStandard": "c99",
            "includePath": [
                "${env:myIncludePath}",
                "${config:idf.espIdfPath}/components/**",
                "${workspaceFolder}/**"
            ],
            "defines": [
                "ESP_PLATFORM"
            ],
            "browse": {
                "path": [
                    "${config:idf.espIdfPath}/components",
                    "${config:idf.espIdfPathWin}/components",
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4,
    "enableConfigurationSquiggles": true
}